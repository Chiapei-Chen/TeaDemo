@model IEnumerable<Product>


<div class="row">
	@foreach(var item in Model)
	{
		<div class="col-lg-3 col-md-6">
		<div class="card-body" style="background-color:cornflowerblue;margin:10px;">
		<div class="p-1">
		<p>@item.Name@item.Size</p>
		
		<p>價格: @item.Price</p>
		</div>
		</div>
		</div>
		

	}


</div>
